# shellcheck  #source
WSROOT="$(x wsroot)"


f()(
    cd "$(x wsroot)" || return 1
    if [ -n "$CI" ]; then
        # Add artifact backup

        # publish bundle
        git add "$(x wsroot)/.x-cmd/dev"
        git commit -m "update pkg available url markdown. by $GITHUB_RUN_NUMBER"

        git push git@gitee.com:x-cmd/pkg main
        git push git@github.com:x-cmd/pkg main
    fi

)

f